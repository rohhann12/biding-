generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  userid Int @id @default(autoincrement())
  amount Int 
  username String @unique
}

model Object{
  objectId Int @id @default(autoincrement())
  name String 
  cost_price Int
  selling_price Int
  images String[]

}